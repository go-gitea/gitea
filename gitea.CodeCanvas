		"4fc53056-f096-48e0-9866-5ce11e58e2d7": {
			"path": "4fc53056-f096-48e0-9866-5ce11e58e2d7",
			"cellName": "Logic: Verifying User Permissions - access.go:L203-220",
			"cellId": "4fc53056-f096-48e0-9866-5ce11e58e2d7",
